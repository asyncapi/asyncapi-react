{
  "name": "@asyncapi/react-component",
  "version": "0.20.1",
  "private": false,
  "description": "A React component for AsyncAPI specification.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asyncapi/asyncapi-react"
  },
  "author": {
    "name": "The Kyma project authors",
    "url": "http://kyma-project.io"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/asyncapi/asyncapi-react/issues"
  },
  "keywords": [
    "asyncapi",
    "asyncapi-specification",
    "react",
    "reactjs",
    "event"
  ],
  "tags": [
    "asyncapi",
    "asyncapi-specification",
    "react",
    "reactjs",
    "event"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "files": [
    "/lib",
    "/bundles",
    "/styles",
    "./README.md",
    "./LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "start": "tsc -p tsconfig.esm.json --watch",
    "build:dev": "npm run build:esm && npm run build:styles",
    "build:prod": "npm run build:esm && npm run build:cjs && npm run build:umd && npm run build:types && npm run build:styles && npm run build:styles:prod",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:umd": "webpack --colors",
    "build:types": "tsc -p tsconfig.types.json",
    "build:styles": "postcss src/styles/default.css -o styles/default.css",
    "build:styles:prod": "cross-env NODE_ENV=production postcss src/styles/default.css -o styles/default.min.css",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --detectOpenHandles --watch",
    "prepare": "npm run build:prod",
    "prepack": "cp ../README.md ./README.md && cp ../LICENSE ./LICENSE",
    "postpack": "rm -rf ./README.md && rm -rf ./LICENSE"
  },
  "dependencies": {
    "@asyncapi/avro-schema-parser": "^0.2.0",
    "@asyncapi/openapi-schema-parser": "^2.0.0",
    "@asyncapi/parser": "^1.4.5",
    "constate": "^1.2.0",
    "isomorphic-dompurify": "^0.13.0",
    "marked": "^2.0.3",
    "openapi-sampler": "^1.0.0-beta.18",
    "react-use": "^12.2.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.4.0",
    "@types/dompurify": "^2.0.4",
    "@types/jest": "^24.0.18",
    "@types/marked": "^2.0.2",
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.2",
    "autoprefixer": "^10.2.5",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.11",
    "jest": "^24.9.0",
    "postcss": "^8.2.10",
    "postcss-cli": "^8.3.1",
    "react": "^16.9.0",
    "tailwindcss": "^2.1.1",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.1.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^3.3.12"
  },
  "publishConfig": {
    "access": "public"
  }
}
