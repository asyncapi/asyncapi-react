{
  "name": "playground",
  "version": "0.2.0",
  "private": true,
  "description": "Playground app for AsyncApi React Component",
  "homepage": "https://www.asyncapi.com/asyncapi-react",
  "dependencies": {
    "@kyma-project/asyncapi-react": "0.2.0",
    "codemirror": "^5.41.0",
    "re-resizable": "^4.10.0",
    "react": "^16.6.3",
    "react-codemirror2": "^5.1.0",
    "react-dom": "^16.6.3",
    "styled-components": "^4.0.3"
  },
  "devDependencies": {
    "@types/codemirror": "0.0.69",
    "@types/jest": "23.3.9",
    "@types/node": "10.12.9",
    "@types/react": "16.4.18",
    "@types/react-dom": "16.0.9",
    "@types/styled-components": "^4.0.3",
    "react-app-rewired": "^1.6.2",
    "react-scripts": "2.1.1",
    "typescript": "3.1.6",
    "gh-pages": "^2.0.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "publish:origin": "npm run build && gh-pages -d build -m 'Deploy new version of playground app' -o origin",
    "publish:upstream": "npm run build && gh-pages -d build -m 'Deploy new version of playground app' -o upstream"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
